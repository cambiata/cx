<?xml version="1.0" encoding="utf-8"?>
<project>
	<!-- NMML reference: https://gist.github.com/1763850 -->
	
	<!-- metadata, make sure 'package' is at least 3 segments (ie. com.mycompany.myproject) -->
	<meta title="MediaLoading" package="scorx.media.main.test.MediaLoading" version="1.0.0" company="" />
	
	<!-- output -->
	<app main="scorx.media.main.test.Main" file="MediaLoading" path="bin" />
	
	<window background="#ffffff" fps="60" />
	<window width="720" height="600" unless="mobile" />
	<window orientation="landscape" vsync="false" antialiasing="0" if="cpp" />
	
	<!-- classpath, haxe libs -->
	<source path="src" />
	<source path="F:/_cx/src" />
	<haxelib name="openfl" />
	<haxelib name="actuate" />
	<haxelib name="stablexui" />
	<haxelib name="mloader" />
	
	<!-- assets -->
	<icon path="assets/openfl.svg" />
	<assets path="assets/img" rename="img" />
	
	<!-- optimize output -->
	<haxeflag name="-dce std" if="haxe3" />
	
</project>